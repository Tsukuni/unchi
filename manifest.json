{
  "name": "unchi-buriburi",
  "version": "1.0",
  "description": "うんこっぽい文字を検出したら後ろに絵文字を追加します",
  "permissions": ["http://*/*", "https://*/*"],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["index.js"]
    }
  ],
  "icons": {
    "16": "images/unchi16.png",
    "32": "images/unchi32.png",
    "48": "images/unchi48.png",
    "128": "images/unchi128.png"
  },
  "manifest_version": 2
}
